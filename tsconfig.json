{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "baseUrl": "./src",
    "paths": {
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@interfaces/*": ["interfaces/*"],
      "@db/*": ["dbqueries/*"],
      "@reducers/*": ["reducers/*"]
    }
  },
  "exclude": ["node_modules", "dist", "out", ".webpack"]
}
